package main

import "fmt"

type Vertex struct {
    Lat, Long float64
}

var m = map[string]Vertex{
    "Bell Labs": Vertex{
        40.68433, -74.39967,
    },
    "Google": Vertex{
        37.42202, -122.08408,
    },
}

var noVertex = map[string]Vertex{
		"one":{12.34,98.64},
		"two":{34.54,56.45},
		}

func main() {
    fmt.Println(m)
    fmt.Println(noVertex)
}
